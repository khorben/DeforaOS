#: ../src/lockerctl.c:29
msgid ""
"Usage: lockerctl -l\n"
"       lockerctl -s\n"
"       lockerctl -u\n"
"  -l\tLock the screen\n"
"  -s\tEnable the screen saver\n"
"  -u\tUnlock the screen\n"
msgstr ""
"Usage: lockerctl -l\n"
"       lockerctl -s\n"
"       lockerctl -u\n"
"  -l\tVerrouiller l'écran\n"
"  -s\tActiver l'économiseur d'écran\n"
"  -u\tDéverrouiller l'écran\n"

#: ../src/main.c:44
msgid ""
"Usage: locker [-p plug-in][-s]\n"
"  -p\tAuthentication plug-in to load\n"
"  -s\tSuspend automatically when locked\n"
msgstr ""
"Usage locker [-p greffon][-s]\n"
"  -p\tGreffon d'authentification à charger\n"
"  -s\tSuspendre automatiquement au verrouillage\n"

