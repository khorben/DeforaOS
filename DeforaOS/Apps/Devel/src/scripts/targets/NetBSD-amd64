#includes
. "`dirname $0`/Apps/Devel/src/scripts/targets/NetBSD"


#functions
#public
#_image_post
_image_post()
{
	case "$IMAGE_TYPE" in
		"iso")
			_image_iso_post
			;;
		*)
			_image_image_post
			;;
	esac
}


#_image_pre
_image_pre()
{
	case "$IMAGE_TYPE" in
		"iso")
			_image_iso_pre
			;;
		*)
			_image_image_pre
			;;
	esac
}
